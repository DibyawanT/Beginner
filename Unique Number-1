/*This is the DCB on 3rd Feb 2020 url->https://hack.codingblocks.com/app/dcb/462*/
#include<stdio.h>
int main(void) {
	int i,j,n;
	int result=0;
	scanf("%d",&n);
	int a[n];
	for(i=0;i<n;i++){
		scanf("%d",&a[i]);
	}
		for(int i=0; i<n; ++i){
			int x = 1<<i;
			int sum = 0;
			for(int j=0; j<n; ++j){
				if((a[j]&x) !=0 )
					sum++;
			}
			
			if(sum%2!=0)
				result |= x;
		}
	printf("%d",result);
	return 0;
